<MainAdmin>:
  name : "MainAdmin"

  GridLayout:
    rows : 2
    GridLayout:
      size_hint : 1,0.05
      cols : 3

      Button:
        text : "Back"
        background_color : 0,1,0,0.7
        size_hint : 0.15,1
        on_press : root.manager.current="LoginAdmin"

      MDCard:
        size_hint:
        md_bg_color : [0/255,0/255,0/255,255/255]
