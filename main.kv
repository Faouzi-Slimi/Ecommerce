      MDCard :
        size_hint  : 0.4,1

        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        size: 400, 500
        md_bg_color : [100/255,170/255,150/255,255/255]
        elevation: 10
        padding: 65
        spacing: 15
        orientation: 'vertical'

        MDIcon:
          icon: 'account'
          icon_color: 0, 0, 0, 0
          halign: 'center'
          font_size: 200
        MDTextFieldRound:
          id: firstname
          icon_left: "account-check"
          hint_text: "First Name"
          foreground_color: 0, 0, 0, 1
          size_hint_x: None
          width: 220
          font_size: 20
          pos_hint: {"center_x": 0.5}
        MDTextFieldRound:
          id: lastname
          icon_left: "account-check"
          hint_text: "Last Name"
          foreground_color: 0, 0, 0, 1
          size_hint_x: None
          width: 220
          font_size: 20
          pos_hint: {"center_x": 0.5}
        MDTextFieldRound:
          id: cin
          icon_left: "account-check"
          hint_text: "Cin Number"
          input_filter : 'float'
          foreground_color: 0, 0, 0, 1
          size_hint_x: None
          width: 220
          font_size: 20
          pos_hint: {"center_x": 0.5}
        MDTextFieldRound:
          id: username
          icon_left: "account-check"
          hint_text: "UserName"
          foreground_color: 0, 0, 0, 1
          size_hint_x: None
          width: 220
          font_size: 20
          pos_hint: {"center_x": 0.5}

        MDTextFieldRound:
          id : password
          icon_left: "key-variant"
          hint_text: "Password"
          foreground_color: 0, 0, 0, 1
          size_hint_x: None
          width: 220
          font_size: 20
          pos_hint: {"center_x": 0.5}
          password : True
        MDFillRoundFlatButton:
          id : adduser

          text : "Add User"
          pos_hint: {"center_x": 0.5}
          md_bg_color : 0,0,1,0.7
          on_press : app.add_user()
        MDFillRoundFlatButton:
          text : "Remove User"
          pos_hint: {"center_x": 0.5}
          md_bg_color : 0,0,1,0.7
          on_press : app.delete_user()


      GridLayout:
        size_hint : 1,1
        rows : 4
        MDCard :
          size_hint :1,0.1
          md_bg_color : [150/255,170/255,100/255,255/255]
