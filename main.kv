ScreenManager
  Login:
  LoginAdmin:
  LoginUser :
  MainAdmin  :
  MainUser :





<Login>:
  name : "Login"
  MDCard:
    size_hint: None, None
    size: 300, 450
    pos_hint: {"center_x": 0.5, "center_y": 0.5}
    elevation: 10
    padding: 65
    spacing: 15
    orientation: 'vertical'


    MDIcon:
      icon: 'account'
      icon_color: 0, 0, 0, 0
      halign: 'center'
      font_size: 250
    MDLabel:
      text : "\n\nLogin For"
      halign: 'center'
    MDFillRoundFlatButton:
      text: "Admin"
      theme_text_color: "Custom"
      on_press :
        root.manager.current = "LoginAdmin"
      line_color: 0, 0, 1, 1
      pos_hint: {"center_x": 0.5, "center_y": 0.5}
    MDFillRoundFlatButton:
      id : true
      text: "User"
      bool : "True"
      theme_text_color: "Custom"
      on_press :

        root.manager.current = "LoginUser"

      line_color: 0, 0, 1, 1
      pos_hint: {"center_x": 0.5, "center_y": 0.5}
    MDLabel:
      pos_hint: {"center_x": 0.5, "center_y": 0.5}
      text : "WELCOME"
      halign: 'center'



<LoginAdmin>:
  name : "LoginAdmin"

  MDCard:
    size_hint: None, None
    size: 400, 500
    pos_hint: {"center_x": 0.5, "center_y": 0.5}
    elevation: 10
    padding: 65
    spacing: 15
    orientation: 'vertical'
    MDIcon:
      icon: 'account'
      icon_color: 0, 0, 0, 0
      halign: 'center'
      font_size: 200


    MDTextFieldRound:
      id: adname
      icon_left: "account-check"
      hint_text: "Username"
      foreground_color: 0, 0, 0, 1
      size_hint_x: None
      width: 220
      font_size: 20
      pos_hint: {"center_x": 0.5}

    MDTextFieldRound:
      id: adpassword
      icon_left: "key-variant"
      hint_text: "Password"
      foreground_color: 0, 0, 0, 1
      size_hint_x: None
      width: 220
      font_size: 20
      pos_hint: {"center_x": 0.5}
      password : True
    MDFillRoundFlatButton:
      id : admn
      bool : "True"
      text : "Login"
      pos_hint: {"center_x": 0.5}
      md_bg_color : 1,0,0,0.7
      on_press : root.manager.current = "LoginUser"


    MDFillRoundFlatButton:
      text : "Back"
      pos_hint: {"center_x": 0.5}
      md_bg_color : 0.6,1,0.6,0.7
      on_press : root.manager.current="Login"
